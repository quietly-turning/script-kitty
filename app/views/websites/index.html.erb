<h1>websites</h1>
<p><%= paginate @websites %></p>


<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Url</th>
      <th>Institution</th>
      <th colspan="3">manage</th>
    </tr>
  </thead>

  <tbody>
    <% @websites.each do |website| %>
      <tr>
        <td><%= website.classification %></td>
        <td><%= website.url %></td>
        <td><%= website.institution.name %></td>
        <td><%= link_to 'Show', website %></td>
        <td><%= link_to 'Edit', edit_website_path(website) %></td>
        <td><%= link_to 'Destroy', website, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Website', new_website_path %>

