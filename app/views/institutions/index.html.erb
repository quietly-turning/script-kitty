<h1>Institutions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Chief</th>
      <th>Control ID</th>
      <th>Level ID</th>
      <th>Locale ID	</th>
      <th colspan="3">manage</th>
    </tr>
  </thead>

  <tbody>
    <% @institutions.each do |institution| %>
      <tr>
        <td><%= institution.name %></td>
        <td><%= institution.city %></td>
        <td><%= institution.state %></td>
        <td><%= institution.zip %></td>
        <td><%= institution.chief %></td>
        <td><%= institution.control.id %></td>
        <td><%= institution.level.id %></td>
        <td><%= institution.locale.id %></td>
        <td><%= link_to 'Show', institution %></td>
        <td><%= link_to 'Edit', edit_institution_path(institution) %></td>
        <td><%= link_to 'Destroy', institution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Institution', new_institution_path %>

<p><%= paginate @institutions %></p>